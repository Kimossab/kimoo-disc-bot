{
  "name": "kimoo-disc-bot",
  "version": "2.6.0",
  "description": "Kimossab's Discord Bot",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/bot.ts",
    "test": "jest --maxWorkers=1",
    "coverage": "jest --coverage --detectOpenHandles --maxWorkers=1",
    "prod": "node ./build/bot.js",
    "lint": "eslint ./src --ext .ts,.json",
    "lint:fix": "eslint ./src --ext .ts,.json --fix",
    "type:check": "tsc --noEmit --skipLibCheck",
    "type:check:watch": "tsc --noEmit --skipLibCheck -w",
    "format": "prettier --write '**.*{ts,json}'",
    "pre-commit": "lint-staged",
    "postinstall": "husky install"
  },
  "author": "Kimossab",
  "license": "ISC",
  "devDependencies": {
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@types/jest": "^29.5.1",
    "@types/request": "^2.48.8",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "casual": "^1.6.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "mongodb-memory-server": "^8.12.2",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "canvas": "^2.11.2",
    "dotenv": "^16.0.3",
    "fast-average-color-node": "^2.6.0",
    "form-data": "^4.0.0",
    "mongoose": "^7.2.2",
    "winston": "^3.8.2",
    "winston-loki": "^6.0.6",
    "ws": "^8.13.0"
  },
  "lint-staged": {
    "*.{tsx,ts,json}": [
      "prettier --write",
      "eslint --fix --ext .ts,.tsx,.json"
    ]
  }
}
