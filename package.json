{
  "name": "kimoo-disc-bot",
  "version": "2.5.4",
  "description": "Kimossab's Discord Bot",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only ./src/bot.ts",
    "test": "jest",
    "coverage": "jest --coverage --detectOpenHandles",
    "prod": "node ./build/bot.js",
    "lint": "eslint . --ext .ts,.json",
    "lint:fix": "eslint . --ext .ts,.json --fix",
    "type:check": "tsc --noEmit --skipLibCheck",
    "type:check:watch": "tsc --noEmit --skipLibCheck -w",
    "format": "prettier --write '**.*{ts,json}'",
    "pre-commit": "lint-staged",
    "postinstall": "husky install"
  },
  "author": "Kimossab",
  "license": "ISC",
  "devDependencies": {
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@types/jest": "^29.2.5",
    "@types/request": "^2.48.7",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-deprecation": "^1.3.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "mongodb-memory-server": "^8.11.0",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "axios": "^1.2.2",
    "canvas": "^2.11.0",
    "dotenv": "^16.0.3",
    "fast-average-color-node": "^2.6.0",
    "form-data": "^4.0.0",
    "mongoose": "^6.0.8",
    "ws": "^8.11.0"
  },
  "lint-staged": {
    "*.{tsx,ts,json}": [
      "prettier --write",
      "eslint --fix --ext .ts,.tsx,.json"
    ]
  }
}
