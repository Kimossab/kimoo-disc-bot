{
  "name": "kimoo-disc-bot",
  "version": "2.19.0",
  "description": "Kimossab's Discord Bot",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/bot.ts",
    "prod": "node ./build/bot.js",
    "lint": "eslint ./src --ext .ts,.json",
    "lint:fix": "eslint ./src --ext .ts,.json --fix",
    "type:check": "tsc --noEmit --skipLibCheck",
    "type:check:watch": "tsc --noEmit --skipLibCheck -w",
    "migrate:prod": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "discord-types-gen": "openapi-ts"
  },
  "author": "Kimossab",
  "license": "ISC",
  "devDependencies": {
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@stylistic/eslint-plugin": "^2.11.0",
    "@types/request": "^2.48.8",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.18.0",
    "discord-api-types": "^0.37.103",
    "lint-staged": "^15.2.10",
    "prisma": "^5.7.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.4.2",
    "@prisma/client": "^5.7.1",
    "axios": "^1.7.7",
    "dotenv": "^16.3.1",
    "form-data": "^4.0.0",
    "winston": "^3.10.0",
    "winston-loki": "^6.0.6",
    "ws": "^8.17.1"
  }
}
