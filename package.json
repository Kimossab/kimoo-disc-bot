{
  "name": "kimoo-disc-bot",
  "version": "2.19.0",
  "description": "Kimossab's Discord Bot",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && tsc-alias -p tsconfig.build.json",
    "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only ./src/bot.ts",
    "prod": "node ./build/bot.js",
    "lint": "eslint ./src --ext .ts,.json",
    "lint:fix": "eslint ./src --ext .ts,.json --fix",
    "type:check": "tsc --noEmit --skipLibCheck",
    "type:check:watch": "tsc --noEmit --skipLibCheck -w",
    "format": "prettier --write '**.*{ts,json}'",
    "pre-commit": "lint-staged",
    "postinstall": "husky install"
  },
  "author": "Kimossab",
  "license": "ISC",
  "devDependencies": {
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@types/request": "^2.48.8",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "casual": "^1.6.2",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.0",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "eslint-import-resolver-typescript": "^3.5.5",
    "form-data": "^4.0.0",
    "mongoose": "^7.4.1",
    "winston": "^3.10.0",
    "winston-loki": "^6.0.6",
    "ws": "^8.13.0"
  },
  "lint-staged": {
    "*.{tsx,ts,json}": [
      "prettier --write",
      "eslint --fix --ext .ts,.tsx,.json"
    ]
  }
}
